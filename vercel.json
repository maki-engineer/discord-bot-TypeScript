{
    "version": 2,
    "builds": [
        {
            "src": "build/src/main.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/.*",
            "dest": "/build/src/main.js",
            "methods": ["GET", "POST"]
        }
    ]
}
